  "scripts": {
    "deploy": "sst deploy --stage production",
    "dev": "sst dev",
    "format": "pnpm -r format --log-level error",
    "lint": "pnpm -r lint",
    "lint-staged": "pnpm -r --workspace-concurrency=1 exec lint-staged",
    "prepare": "husky && sst install && sst telemetry disable",
